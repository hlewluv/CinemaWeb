                                    <i class="ti-arrow-circle-down"></i>
                                </a>
                            </div>
                        </div>
                        <div class="collapse show" id="movielist">
                            <div class="movie-list py-1">
                                <div class="row">
                                    @if (ViewBag.MovieList != null)
                                    {
                                        foreach (CinemaWeb.Models.movy movie in ViewBag.MovieList)
                                        {
                                            if (movie.movie_status == true)
