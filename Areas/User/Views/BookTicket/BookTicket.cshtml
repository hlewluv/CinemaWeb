                                </a>
                            </div>
                        </div>
                        <div class="collapse day-cinema" id="datetime">
                            <div class="datetime-wrapper">
                                <div class="datetime-container" role="tabpanel">
                                    <div class="row align-items-center date-list">
                                        <ul class="list-group list-group-horizontal date-list-side">
                                            @for (var date = currentDate; date <= currentDate.AddDays(6); date = date.AddDays(1))
                                            {
                                                <li class="col-md list-group-item date-time-item" id="@($"date-{date.ToString("yyMMdd")}-li")">
                                                    <a class="text-dark-emphasis fw-medium" onclick="selectDate('@date.ToString("yyMMdd")')" href="@($"#date-{date.ToString("yyMMdd")}")" data-bs-toggle="list" data-bs-target="@($"#date-{date.ToString("yyMMdd")}")" role="tab">
                                                        <span>@date.DayOfWeek</span>
                                                        <span>@date.ToString("dd/MM")</span>
                                                    </a>
                                                </li>
                                            }
                                        </ul>
                                    </div>
                                    <div class="tab-content schedule-cinema">
                                        <div class="row">
                                            <h2 class="movie-text cinema-name">Ohayou Đà Nẵng</h2>
                                        </div>
                                        <div class="tab-pane time-cinema active" id="@($"date-{currentDate.ToString("yyMMdd")}")" role="tabpanel">
                                            <div class="row align-items-center time-list">
                                                <ul class="list-group list-group-horizontal justify-content-center">
                                                    @for (var hour = currentHour + 1; hour <= 24; hour++)
                                                    {
                                                        if (total == 6) { break; }
                                                        <li class="col-md-1 list-group-item time-btn">
                                                            <a class="text-dark-emphasis fw-medium" href="@($"#hour-{hour.ToString().PadLeft(2, '0')}")" data-bs-toggle="list" data-bs-target="@($"#hour-{hour.ToString().PadLeft(2, '0')}")" role="tab">
                                                                <span class="btn btn-outline-secondary time-item">@hour.ToString().PadLeft(2, '0'):00</span>
                                                            </a>
                                                        </li>
                                                        total++;
                                                    }
